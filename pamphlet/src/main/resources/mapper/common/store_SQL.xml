<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
<mapper namespace="common.store">
     <select id="selectAllStore" resultType="hashmap">
		<![CDATA[
			SELECT store_no
			         ,store_name
             FROM mega_store
           ORDER BY store_no
		]]>
	</select>
</mapper>